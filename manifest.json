{
  "manifest_version": 2,
  "name": "Mafia Assets",
  "description": "Just a Deeeep.io Asset Pack!",
  "version": "1.0.0",
  "icons": {"128": "icon.png"},
  "background": {"scripts":["background.js"]},
  "browser_action": {
      "default_title": "Click to toggle Mafia Assets",
      "default_popup": "options.html"
  }, 
  "permissions": [
    "webRequest",
    "https://deeeep.io/*",
    "https://mapmaker.deeeep.io/*",
    "https://beta.deeeep.io/*",
    "https://cdn.deeeep.io/*",
    "webRequestBlocking",
    "storage"
  ]
}